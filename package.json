{
  "name": "vue-serverless-ssr-poc",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "unit": "jest",
    "unit:snapshot": "jest --updateSnapshot",
    "deploy": "yarn build && ./node_modules/.bin/sls deploy"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "aws-serverless-express": "^3.2.0",
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "nuxt": "^1.4.1",
    "nuxt-class-component": "^1.0.1",
    "nuxt-property-decorator": "^1.2.0",
    "sweet-modal-vue": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^21.1.4",
    "@types/node": "^9.4.6",
    "babel-jest": "^21.2.0",
    "css-loader": "^1.0.0",
    "jest": "^21.2.1",
    "jest-serializer-vue": "^0.2.0",
    "jest-vue": "^0.8.1",
    "node-sass": "^4.9.2",
    "sass-loader": "^7.0.3",
    "serverless": "^1.28.0",
    "serverless-apigw-binary": "^0.4.4",
    "ts-jest": "^21.1.3",
    "ts-loader": "^3.5.0",
    "tslint": "^5.7.0",
    "typescript": "^2.7.2",
    "vue-loader": "^13.3.0",
    "vue-style-loader": "^4.1.0",
    "vue-test-utils": "^1.0.0-beta.2",
    "vue-typescript-import-dts": "^3.1.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue",
      "ts",
      "tsx"
    ],
    "transform": {
      ".js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "testRegex": "/tests/.*\\.(ts|tsx|js)$",
    "mapCoverage": true
  }
}
